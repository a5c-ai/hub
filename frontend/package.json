    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/showdown": "^2.0.6",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
