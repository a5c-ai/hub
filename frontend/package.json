    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/typography": "^0.5.16",
